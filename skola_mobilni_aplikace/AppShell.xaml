<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="skola_mobilni_aplikace.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:skola_mobilni_aplikace"
    xmlns:views="clr-namespace:skola_mobilni_aplikace.Views"
    Shell.FlyoutBehavior="Disabled">
    
    

    <TabBar>
        <ShellContent
            Title="skola"
            ContentTemplate="{DataTemplate views:Skola}"/>
        <ShellContent
            Title="studenti"
            ContentTemplate="{DataTemplate views:studenti}"/>
        <ShellContent
            Title="udrzbari"
            ContentTemplate="{DataTemplate views:udrzbari}"/>
        <ShellContent
            Title="podpora"
            ContentTemplate="{DataTemplate views:podpora}"/>
        <ShellContent
            Title="ucitele"
            ContentTemplate="{DataTemplate views:ucitele}"/>
    </TabBar>

</Shell>
